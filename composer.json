{
    "name": "jsadways/authenticator",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Js\\Authenticator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
                "Js\\Authenticator\\Providers\\AuthServiceProvider",
                "Js\\Authenticator\\Providers\\UserServiceProvider"
          ],
          "aliases": {
                "JsAuth":  "Js\\Authenticator\\Facades\\AuthFacade",
                "JsUser":  "Js\\Authenticator\\Facades\\UserFacade"
          }
        }
    },
    "authors": [
        {
            "name": "Js-Adways",
            "email": "it@js-adways.com.tw"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0"
    }
}
